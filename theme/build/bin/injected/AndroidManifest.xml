<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.cm.theme"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-feature
		android:required="true"
		android:name="org.cyanogenmod.theme"/>

	<uses-sdk
		android:minSdkVersion="21"
		android:targetSdkVersion="21"/>

	<meta-data
		android:name="org.cyanogenmod.theme.name"
		android:value="@string/theme_name"/>

	<meta-data
		android:name="org.cyanogenmod.theme.author"
		android:value="@string/theme_author"/>

	<application
		android:hasCode="false"
		android:icon="@drawable/ic_launcher"
		android:label="@string/theme_name"/>

</manifest>

